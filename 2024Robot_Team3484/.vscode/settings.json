{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*~": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "files.associations": {
    "matrixfunctions": "cpp",
    "cfloat": "cpp",
    "array": "cpp",
    "compare": "cpp",
    "functional": "cpp",
    "ratio": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "*.inc": "cpp",
    "*.tcc": "cpp",
    "optional": "cpp",
    "system_error": "cpp",
    "regex": "cpp",
    "memory": "cpp",
    "variant": "cpp",
    "ranges": "cpp"
  },
  "java.project.sourcePaths": [
    "build"
  ],
  "C_Cpp.errorSquiggles": "enabled"
}
